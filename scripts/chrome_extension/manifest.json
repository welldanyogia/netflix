{
  "manifest_version": 3,
  "name": "Widevine Key Interceptor",
  "version": "1.0",
  "description": "Educational: Intercept Widevine EME calls",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
